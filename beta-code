              <h2>Choose a theme:</h2>
              <br>
                  <button id="device_theme" onclick="deviceTheme()">Device Theme &#10003;</button>
                  <button id="dark_mode" onclick="darkMode();">Dark Mode</button>
                  <button id="light_mode" onclick="lightMode()">Light Mode</button>
                <script>
                  function deviceTheme() {
                    document.getElementById('device_theme').innerHTML = "Device Theme&#10003;";
                     document.getElementById('dark_mode').innerHTML = "Dark Mode";
                    document.getElementById('light_mode').innerHTML = "Light Mode";
                    
                  }
                  
                  function darkMode() {
                    document.getElementById('dark_mode').innerHTML = "Dark Mode &#10003;";
                    document.getElementById('device_theme').innerHTML = "Device Theme";
                    document.getElementById('light_mode').innerHTML = "Light Mode";
                    document.getElementById('menu').style.background = "#000";
                    document.getElementsByTagName('button').style.background = "#202124";
                    document.getElementsByTagName('button').style.color = "#fff";
                    document.getElementsByTagName('a').style.color = "#fff";
                    document.getElementById('font').style.background = "#000";
                    document.getElementById('font').style.color = "#fff";
                    window.localStorage.setItem("darkMode");
                    window.localStorage.removeItem("deviceTheme");
                  }  
                  
                    function lightMode() {
                    document.getElementById('dark_mode').innerHTML = "Dark Mode";
                    document.getElementById('device_theme').innerHTML = "Device Theme";
                    document.getElementById('light_mode').innerHTML = "Light Mode &#10003;";
                    document.getElementById('menu').style.background = "#fff";
                    document.getElementsByTagName('a').style.color = "#000";
                    document.getElementById('font').style.background = "#fff";
                    document.getElementById('font').style.color = "#000";
                    document.getElementById('language').style.background = "#fff";
                    document.getElementById('ul').style.background = "#fff"
                    document.getElementById('footer').style.background = "#fff"
                    document.cookie = "lightMode=wqeiruhq; expires=Thur, 1 Jan 00:00:00 GMT; Secure";
                  }
                </script>
            </div>
          </div>
        </div>
      </div>
    </div>
