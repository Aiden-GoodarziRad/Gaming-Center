<script>
document.cookie = "name=gaming_center; SameSite=None; Secure";
document.cookie = "_ga=tripe; SameSite=None; Secure";
 document.cookie = "name=user; SameSite=None; Secure";
document.cookie = "browser_id=(browser_id); SameSite=None; Secure";


function showCookies() {
  const output = document.getElementById('cookies')
  output.textContent = '> ' + document.cookie
}

function clearOutputCookies() {
  const output = document.getElementById('cookies')
  output.textContent = ''
}
</script>
