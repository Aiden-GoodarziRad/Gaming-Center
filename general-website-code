<script>
var myVar;

function loadFunction() {
  myVar = setTimeout(showPage, 100);
}

function showPage() {
  document.getElementById("loader").style.display = "none";
  document.getElementById("myDiv").style.display = "block";
  document.getElementById('wait').style.cursor = "initial";
}
</script>
